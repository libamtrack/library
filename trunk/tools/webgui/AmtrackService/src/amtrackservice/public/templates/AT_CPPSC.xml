<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<root name="RE calculations" wrapper="AT_CPPSC.exe">
    <description>
        [b]CPPSC model calculations[/b]
    </description>
    <input>
        <element type="field" label="fluence [1/cm2]" datatype="double">
            <description>fluence [1/cm2]</description>
            <preset>
                <item value="1e8" />
            </preset>
            <data x="fluence_cm2" />
        </element>   
        <element type="field" label="E [MeV/amu]" datatype="double">
            <description>E [MeV/amu]</description>
            <preset>
                <item value="50" />
            </preset>
            <data x="E_MeV_u" />
        </element>        
        <element type="combo" label="particle" datatype="long">
            <entry>
                <item key="Hydrogen" value="1001" />
                <item key="Helium-3" value="2003" />
                <item key="Helium-4" value="2004" />
                <item key="Berylium" value="4008" />
                <item key="Carbon" value="6012" />
                <item key="Oxygen" value="8016" />
                <item key="Neon" value="10020" />
                <item key="Iron" value="26056" />
                <item key="Xeon" value="54132" />
                <item key="Uranium" value="92238" />
            </entry>
            <description>particle[br]Default value is set to Hydrogen</description>
            <preset>
                <item value="6012" />
            </preset>
            <data x="particle_no" />
        </element>
        <element type="combo" label="material" datatype="long">
            <description>material</description>
            <entry>
                <item key="Liquid Water" value="1" />
                <item key="Aluminum Oxide" value="2" />
                <item key="Aluminum" value="3" />
                <item key="PMMA" value="4" />
                <item key="Alanine" value="5" />
                <item key="LiF" value="6" />
                <item key="Air" value="7" />
            </entry>
            <preset>
                <item value="4" />
            </preset>
            <data x="material_no" />
        </element>      
        
        <element type="combo" label="rdd_model" datatype="long">
            <description>Radial Dose Distribution model</description>
            <entry>
                <item key="Test" value="1" />
                <item key="KatzPoint" value="2" />
                <item key="Geiss" value="3" />
                <item key="KatzSite" value="4" />
                <item key="CucinottaPoint" value="5" />
                <item key="KatzExtTarget" value="6" />
                <item key="CucinottaExtTarget" value="7" />
            </entry>
            <preset>
                <item value="3" />
            </preset>
            <data x="rdd_model" />
        </element>          

        <element type="combo" label="er_model" datatype="long">
            <description>delta electron range model</description>
            <entry>
                <item key="Test" value="1" />
                <item key="ButtsKatz" value="2" />
                <item key="Waligorski" value="3" />
                <item key="Geiss" value="4" />
                <item key="Scholz" value="5" />
                <item key="Edmund" value="6" />
                <item key="Tabata" value="7" />
            </entry>
            <preset>
                <item value="7" />
            </preset>
            <data x="er_model" />
        </element>
        
        <element type="combo" label="gr_model" datatype="long">
            <description>GR model</description>
            <entry>
                <item key="Test" value="1" />
                <item key="GeneralTarget" value="2" />
                <item key="Radioluminescence" value="3" />
                <item key="ExpSaturation" value="4" />
                <item key="LinQuad" value="5" />
                <item key="Geiss" value="6" />
            </entry>
            <preset>
                <item value="5" />
            </preset>
            <data x="gr_model" />
        </element>
        
    </input>

    <output>
        <element type="field" label="efficiency" datatype="double">
            <description>efficiency</description>
            <data x="RE" />
            <preset>
                <item value="1.0" />
            </preset>
        </element>  
        <element type="plot" label="f1" datatype="double">
            <description>f1 histogram</description>
            <data x="f1_d_Gy" y="f1" />
            <axis x="log" y="log" labelx="X" labely="Y"/>
        </element>
        <element type="plot" label="f" datatype="double">
            <description>f histogram</description>
            <data x="fdd" y="f" />
            <axis x="log" y="log" labelx="X" labely="Y"/>
        </element>
    </output>
</root>
