<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<root name="AT_E_from_beta plot" wrapper="AT_E_from_beta.exe">
	<description>
		[b]AT_E_from_beta[/b]
		Returns E as a function of beta
	</description>
	<input>
		<element type="inputlist" label="beta input" datatype="double">
			<description>beta (array of size n)</description>
			<preset>
				<item value="0.1" />
				<item value="0.2" />
				<item value="0.3" />
				<item value="0.6" />
				<item value="0.9" />
			</preset>
			<data x="beta_input" />
		</element>
	</input>

	<output>
		<element type="plot" label="E" datatype="double">
			<description>Energy [MeV/u]</description>
			<data x= "beta_input" y="AT_E_from_beta" />
		</element>
	</output>
</root>
