<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<root name="E from beta" wrapper="AT_E_from_beta.exe">
	<description>
		[b]Returns E as a function of beta[/b]		
	</description>
	<input>
		<element type="inputlist" label="beta" datatype="double">
			<description>beta</description>
			<preset>
				<item value="0.1" />
				<item value="0.2" />
				<item value="0.3" />
				<item value="0.6" />
				<item value="0.9" />
			</preset>
			<data x="beta_input" />
		</element>
	</input>

	<output>
		<element type="plot" label="particle energy" datatype="double">
			<description>Energy [MeV/u]</description>
			<data x= "beta_input" y="E_output" />
		</element>
	</output>
</root>
